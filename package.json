{
  "name": "tests-ex-markdown",
  "version": "2.0.3",
  "description": "Run markdown usage examples as unit tests",
  "main": "index.js",
  "scripts": {
    "prepublish": "lsc --compile --bare --print index.ls | sed '1s:^:#!/usr/bin/env node\\n:' > index.js",
    "test": "lsc index.ls readme.markdown && lsc test.ls"
  },
  "bin": {
    "txm": "index.js"
  },
  "author": "Anko <an@cyan.io>",
  "engines": {
    "node": ">=0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anko/tests-ex-markdown"
  },
  "bugs": "https://github.com/anko/tests-ex-markdown/issues",
  "license": "ISC",
  "files": [
    "index.js",
    "readme.markdown"
  ],
  "keywords": [
    "markdown",
    "test",
    "example",
    "readme",
    "comment",
    "tap"
  ],
  "dependencies": {
    "async": "2.1.1",
    "concat-stream": "2.0.0",
    "minimist": "1.2.0",
    "parse5": "3.0.1",
    "prelude-ls": "1.1.1",
    "remark-parse": "3.0.1",
    "tape": "4.0.0",
    "tapsoneer": "0.1.0",
    "unified": "6.1.3"
  },
  "devDependencies": {
    "livescript": "1.4.0"
  }
}
